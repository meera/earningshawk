{
  "name": "@earninglens/studio",
  "version": "0.1.0",
  "description": "Video production with Remotion for EarningLens earnings videos",
  "private": true,
  "scripts": {
    "start": "remotion studio",
    "render": "remotion render src/index.ts EarningsVideo out/output.mp4",
    "render:aapl": "remotion render src/index.ts EarningsVideo out/AAPL-Q4-2024.mp4 --props='@./data/AAPL-Q4-2024.json'",
    "render:aapl:full": "remotion render src/index.ts EarningsCallVideo out/AAPL-Q4-2024-full.mp4 --props='@./data/AAPL-Q4-2024-FULL.json'",
    "upgrade": "remotion upgrade"
  },
  "dependencies": {
    "@remotion/cli": "^4.0.0",
    "@remotion/player": "^4.0.0",
    "remotion": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0"
  }
}
